<div class="book-image-cont book-part">
  <% if book.images.any? %>
    <img src="<%= book.images.first.url %>" class="book-image">
  <% else %>
    <%= image_tag "/assets/book.png", class: "book-image" %>
  <% end %>
  <div class="image-others">
    <div class="owl-carousel owl-theme">
      <% book.images.each do |i| %>
        <img src="<%= i.url %>" class="book-image-others"
          id="image-other<%= i.id %>">
      <% end %>
    </div>
  </div>
</div>
